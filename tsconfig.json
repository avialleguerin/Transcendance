{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "strict": false,
	  "outDir": "./dist",
	  "rootDir": "./",
	  "baseUrl": "./",
	  "sourceMap": true,
	  "allowJs": true,
	},
	"include": [
		"Frontend/public/static/js/views/**/*.ts",
		"Frontend/public/static/js/index.ts"
	],
	"exclude": ["node_modules", "dist"]
}